# Required parameters
REPO_OWNER=GITHUB_USERNAME_HERE
REPO_NAME=your-repository-name
APP_NAME=NextJsApplication
GITHUB_TOKEN=your_github_personal_access_token_here

# Optional parameters with defaults
APP_PATH=frontend
PROD_BRANCH=main
DEV_BRANCH=develop
# Specify a single feature branch name if needed (leave empty if not using)
FEATURE_BRANCH_NAME=feature-branch-name

# Environment variables for API endpoints
PROD_GRAPHQL_URL=https://api.example.com/graphql
DEV_GRAPHQL_URL=https://api-dev.example.com/graphql

# AWS Region (optional, defaults to us-east-1)
CDK_DEFAULT_REGION=us-east-1 
